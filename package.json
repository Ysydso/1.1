{
  "name": "diary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"server,client\" \"yarn server dev\" \"yarn client dev\"",
    "server": "yarn workspace server",
    "client": "yarn workspace client",
    "audit:ci": "audit-ci --high --config=./yarn-audit-ci.config.json"
  },
  "workspaces": [
    "workspaces/*"
  ],
  "devDependencies": {
    "audit-ci": "^5.1.2",
    "concurrently": "^6.4.0"
  }
}
