{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-resolvers": "^1.19.0",
    "apollo-server": "^2.22.2",
    "concurrently": "^5.3.0",
    "graphql": "^15.5.0",
    "sequelize": "^6.3.4"
  },
  "devDependencies": {
    "@types/sequelize": "^4.28.9",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "sqlite3": "^5.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "dev": "concurrently -n \"server,codegen\" \"yarn dev:server\" \"yarn dev:codegen\"",
    "dev:server": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "dev:codegen": "graphql-codegen --watch",
    "test": "jest"
  }
}
